{
  "name": "@nexustech/fingerprint",
  "version": "1.0.0",
  "description": "A browser fingerprinting library for creating persistent, unique and long-lasting digital fingerprints.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsc && node esbuild.js",
    "verify": "eslint './src/**/*.ts' && tsc --noEmit",
    "lint": "eslint './src/**/*.ts'",
    "lint:fix": "eslint './src/**/*.ts' --fix",
    "dev": "ts-node src/test/mock.ts --esm",
    "prepublish": "npm run verify && npm run test && npm run build"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nexus-technolgy/fingerprint.git"
  },
  "keywords": [
    "fingerprinting"
  ],
  "author": "Joe Rutkowski <Joe@dreggle.com>, Daniel Bryar",
  "license": "MIT",
  "devDependencies": {
    "@nexustech/logger": "^1.2.0",
    "@types/mime": "^3.0.1",
    "@types/node": "^20.5.7",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "esbuild": "^0.19.2",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-check-file": "^1.3.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "mime": "^3.0.0",
    "prettier": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.4"
  }
}
